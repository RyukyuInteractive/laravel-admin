{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "repositories": [
     {
      "type":"package",
        "package": {
          "name": "ryukyuInteractive/laravel-admin",
          "version":"1.3.2",
          "source": {
            "url": "https://github.com/RyukyuInteractive/laravel-admin.git",
            "type": "git",
            "reference":"master"
          }
        }
      },
     {
      "type":"package",
        "package": {
          "name": "m-tatsuto/laravel-admin",
          "version":"1.3.2",
          "source": {
            "url": "https://github.com/m-tatsuto/laravel-admin.git",
            "type": "git",
            "reference":"master"
          }
        }
      }
    ],
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "5.3.*",
        "encore/laravel-admin": "1.3.*"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "laravel/homestead": "^4.0",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.0",
        "symfony/css-selector": "3.1.*",
        "symfony/dom-crawler": "3.1.*"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
